{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [{
     "name": "fib-fe",
     "image": "joseluisjb1990/fib-fe:latest",
     "hostname": "client",
     "essential": false,
     "memory": 128
  }, {
    "name": "fib-worker",
    "image": "joseluisjb1990/fib-worker:latest",
    "hostname": "worker",
    "essential": false,
    "memory": 128
 }, {
  "name": "fib-server",
  "image": "joseluisjb1990/fib-server:latest",
  "hostname": "api",
  "essential": false,
  "memory": 128
  }, {
    "name": "fib-nginx",
    "image": "joseluisjb1990/fib-nginx:latest",
    "hostname": "nginx",
    "essential": true,
    "memory": 128,
    "portMappings": [{
      "hostPort": 80,
      "containerPort": 80
    }],
    "links": [
      "fib-fe", 
      "fib-server"
    ]
  }]
}